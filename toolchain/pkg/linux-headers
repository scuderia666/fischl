[build]
%make mrproper
%make headers ARCH=%arch
mkdir -p %tools/%target/include
cp -R usr/include/* %tools/%target/include
find %tools/%target/include -name '.*' -exec rm -rf {} +
rm -f %tools/%target/include/Makefile
